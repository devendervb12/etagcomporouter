<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="smax.etag.COMPROUTE.controller.ProductList" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Product List">
	   
		<content>
	       <List items="{/ProductSet}">
	         <items>
	            <StandardListItem title="{ProductID}" description="{Name}" press="onItemPress"
	            type="Active" />
	         </items>
	       </List>
		</content>
	</Page>
</core:View>